{
  "file_path" : "/etc/mysql/mysql.conf.d/mysqld.cnf",
  "comment_symbol" : "#",
  "permission" : "0644",
  "config": [
    {
      "verb": "add",
      "unique": true,
      "after": "[mysqld]",
      "text": "    sql-mode='STRICT_TRANS_TABLES,NO_ZERO_IN_DATE,NO_ZERO_DATE,ERROR_FOR_DIVISION_BY_ZERO,NO_AUTO_CREATE_USER,NO_ENGINE_SUBSTITUTION'"
    }
  ]
}